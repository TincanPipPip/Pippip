{% for item in items %}
  {% set image = image_style(item.content['#item'].entity.fid.value, ['teaser_thumb_image']) %}
  <img class="lazy" data-src="{{ image.teaser_thumb_image }}" height="660" width="840" alt="{{ item.content['#item'].alt }}" title="{{ item.content['#item'].title }}" />
{% endfor %}